{
  "name": "oko-dashboard",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "node_modules/.bin/vite",
    "build": "node_modules/.bin/vite build && node scripts/build-to-backend.mjs",
    "build:desktop": "OKO_BUILD_TARGET=desktop node_modules/.bin/vite build",
    "tauri:dev": "node_modules/.bin/tauri dev",
    "tauri:build:arm64": "node_modules/.bin/tauri build --target aarch64-apple-darwin",
    "tauri:build:universal": "node_modules/.bin/tauri build --target universal-apple-darwin",
    "preview": "node_modules/.bin/vite preview --port 4173",
    "lint": "node_modules/.bin/eslint \"src/**/*.{js,vue}\" --max-warnings=0",
    "typecheck:services": "node_modules/.bin/tsc -p tsconfig.typecheck.services.json --noEmit",
    "typecheck:app": "node_modules/.bin/vue-tsc -p tsconfig.typecheck.app.json --noEmit",
    "typecheck": "npm run typecheck:services && npm run typecheck:app",
    "test": "node --test ../../tests/frontend/*.test.mjs",
    "check:files": "node scripts/check-file-length.mjs"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.9.0",
    "@tauri-apps/api": "^2.8.0",
    "@vueuse/motion": "^3.0.3",
    "lucide-vue-next": "^0.575.0",
    "simple-icons": "^16.9.0",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@tauri-apps/cli": "^2.8.0",
    "eslint": "^9.38.0",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^16.5.0",
    "sass": "^1.97.3",
    "typescript": "^5.9.3",
    "vite": "^5.4.8",
    "vue-tsc": "^2.2.12",
    "web-vitals": "^5.1.0"
  }
}
