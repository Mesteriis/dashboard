{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "strict": true,
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "types": ["vite/client"]
  },
  "include": ["src/**/*.js", "src/**/*.vue", "src/**/*.d.ts"]
}
